{
  "name": "code2know",
  "private": true,
  "version": "0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "api": "nodemon --exec node --loader ts-node/esm api/server.ts",
    "test": "echo Testes ainda nao implementados",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "cm": "cz"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@phosphor-icons/react": "^2.0.5",
    "@types/crypto-js": "^4.1.1",
    "@uiw/react-textarea-code-editor": "^2.1.1",
    "antd": "^5.3.2",
    "axios": "^1.3.4",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "firebase": "^9.19.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-joyride": "^2.5.4",
    "react-router-dom": "^6.9.0",
    "react-toastify": "^9.1.1",
    "react-vertical-timeline-component": "^3.6.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@types/json-server": "^0.14.4",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-vertical-timeline-component": "^3.3.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vitejs/plugin-react": "^3.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.33.0",
    "eslint-plugin-react": "^7.32.2",
    "husky": "^8.0.3",
    "json-server": "^0.17.1",
    "nodemon": "^2.0.20",
    "typescript": "^4.9.3",
    "vite": "^4.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog",
      "types": {
        "🌟 feat": {
          "description": "Um novo recurso",
          "title": "Recurso"
        },
        "🔧 fix": {
          "description": "Uma correção de um bug",
          "title": "Consertos"
        },
        "📖 docs": {
          "description": "Apenas a documentação muda",
          "title": "Documentação"
        },
        "💄 style": {
          "description": "Alterações que não afetam o significado do código (espaço em branco, formatação, falta de ponto-e-vírgula etc.)",
          "title": "Estilo"
        },
        "📈 perf": {
          "description": "Uma mudança de código que melhora o desemprenho",
          "title": "Performance"
        },
        "🧪 test": {
          "description": "Adicionando testes ausentes ou corrigindo testes existentes",
          "title": "Teste"
        },
        "⚙️ build": {
          "description": "Alterações que afetam o sistema de compilação ou dependências externas (exemplo: escopos: gulp, broccoli, npm)",
          "title": "Build"
        },
        "🔁 ci": {
          "description": "Alterações em nossos arquivos e scripts de configuração de CI (exemplos de escopos: Travis, Circle, BrowserStack, SauceLabs)",
          "title": "Integração Continua"
        },
        "❓ chore": {
          "description": "Outras alterações que não modificam os arquivos src ou de teste",
          "title": "Tarefa"
        },
        "🔀 reverter": {
          "description": "Reverte um commit anterior",
          "title": "Reverter"
        }
      }
    }
  }
}
